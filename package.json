{
  "private": true,
  "name": "react-dnd-bug-showcase",
  "version": "1.0.0",
  "description": "A sample React project",
  "repository": {
    "type": "git",
    "url": "https://github.com/catamphetamine/react-dnd-bug-showcase.git"
  },
  "homepage": "https://github.com/catamphetamine/react-dnd-bug-showcase",
  "scripts": {
    "start": "npm-run-all development:client:rendering-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:services",
    "development:client:rendering-service:build:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "development:client:build": "webpack-dev-server --hot --config webpack/webpack.config.client.development.babel.js",
    "development:client:rendering-service:build": "webpack --mode development --config ./webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:client:rendering-service": "nodemon ./rendering-service/index.entry.js --watch ./rendering-service --watch ./build/server --no-update-notifier",
    "development:services": "npm-run-all --parallel development:client:rendering-service"
  },
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-website": "^3.0.4",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-calc": "^7.0.1",
    "postcss-comment": "^2.0.0",
    "postcss-custom-properties": "^8.0.9",
    "postcss-import": "^12.0.1",
